{
  "name": "v-aos",
  "version": "0.0.3-a1",
  "description": "Vue plugin for AOS (Animate on scroll library)",
  "main": "dist/v-aos.umd.js",
  "module": "dist/v-aos.esm.js",
  "unpkg": "dist/v-aos.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vue-cli-service serve dev/index.js",
    "prepare": "npm run build",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "precommit": "lint-staged",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd --file dist/v-aos.umd.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es --file dist/v-aos.esm.js",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife --file dist/v-aos.min.js",
    "test": "exit 0;"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ],
    "examples/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikhailsidorov/v-aos.git"
  },
  "keywords": [
    "vue",
    "aos",
    "v-aos",
    "animate on scoll"
  ],
  "author": "Mikhail Sidorov <me@mikhailsidorov.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikhailsidorov/v-aos/issues"
  },
  "homepage": "https://github.com/mikhailsidorov/v-aos#readme",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@vue/cli-service": "^4.5.12",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.21",
    "postcss": "^8.2.10",
    "prettier": "^1.19.1",
    "rollup": "^2.45.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "vue": "^2.6.12"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6"
  }
}
